<div class = "user-index-wrapper">
  <div class = "user-index-title" ><b>登録者一覧</b></div>
  <%= link_to 'マイページへ戻る', user_path(current_user.id) %>
  <br>
    <% @users.each do |user|%>
    <% if user.id == current_user.id %>
      <% next %>
    <% else %>
      <%= link_to user.nickname, user_path(user.id)%>
    <% end %>
    <br>
  <% end %>
</div>